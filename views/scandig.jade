extends layout

block content
  h1= title
  p Welcome to #{title}

  p Scanning digits: #{digits}
  
  if mode
    p Mode is set to #{mode}
    button#button1 GO!

  script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js")
  script.
    var digits = "#{digits}";
  script(src="/javascripts/procdigcmu.js")

  table#results_table(border='1')
    tr
      th Col 1
      th Col 2

